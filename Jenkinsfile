pipeline {
  agent any
  stages {
    stage('Build Backend') {
      steps {
        echo 'Hello'
      }
    }
    stage('Test Backend') {
      steps {
        echo 'testing'
      }
    }
    stage('More Tests') {
      steps {
        echo 'more tests'
      }
    }
    stage('Deploy Staging') {
      steps {
        echo 'deploying'
      }
    }
    stage('Deploy Production') {
      steps {
        echo 'prod'
      }
    }
  }
}